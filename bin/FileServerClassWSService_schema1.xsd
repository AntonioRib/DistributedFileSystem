<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" targetNamespace="http://ws.fileServer.server/" xmlns:tns="http://ws.fileServer.server/" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <xs:element name="IOException" type="tns:IOException"/>

  <xs:element name="InfoNotFoundException" type="tns:InfoNotFoundException"/>

  <xs:element name="dir" type="tns:dir"/>

  <xs:element name="dirResponse" type="tns:dirResponse"/>

  <xs:element name="getFile" type="tns:getFile"/>

  <xs:element name="getFileInfo" type="tns:getFileInfo"/>

  <xs:element name="getFileInfoResponse" type="tns:getFileInfoResponse"/>

  <xs:element name="getFileResponse" type="tns:getFileResponse"/>

  <xs:element name="getHost" type="tns:getHost"/>

  <xs:element name="getHostResponse" type="tns:getHostResponse"/>

  <xs:element name="getName" type="tns:getName"/>

  <xs:element name="getNameResponse" type="tns:getNameResponse"/>

  <xs:element name="makeDir" type="tns:makeDir"/>

  <xs:element name="makeDirResponse" type="tns:makeDirResponse"/>

  <xs:element name="receiveFile" type="tns:receiveFile"/>

  <xs:element name="receiveFileResponse" type="tns:receiveFileResponse"/>

  <xs:element name="removeFile" type="tns:removeFile"/>

  <xs:element name="removeFileResponse" type="tns:removeFileResponse"/>

  <xs:element name="sendFile" type="tns:sendFile"/>

  <xs:element name="sendFileResponse" type="tns:sendFileResponse"/>

  <xs:complexType name="sendFile">
    <xs:sequence>
      <xs:element name="arg0" type="xs:string" minOccurs="0"/>
      <xs:element name="arg1" type="xs:string" minOccurs="0"/>
      <xs:element name="arg2" type="xs:boolean"/>
      <xs:element name="arg3" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="sendFileResponse">
    <xs:sequence>
      <xs:element name="return" type="xs:boolean"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="IOException">
    <xs:sequence>
      <xs:element name="message" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="getName">
    <xs:sequence/>
