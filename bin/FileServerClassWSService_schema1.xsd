  <xs:complexType name="getNameResponse">
    <xs:sequence>
      <xs:element name="return" type="xs:string" minOccurs="0"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="removeFile">
    <xs:sequence>
      <xs:element name="arg0" type="xs:string" minOccurs="0"/>
